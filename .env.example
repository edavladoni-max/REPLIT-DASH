# Core app
PORT=5000
NODE_ENV=development
SESSION_SECRET=change-me

# VPS proxy
VPS_VL=user@host:password
VPS_API_PORT=8080
DASHBOARD_PASSWORD=change-me

# Persistent command queue
DATABASE_URL=postgresql://dashboard:change-me@127.0.0.1:5432/dashboard?sslmode=disable

# MemOS auto-context
MEMOS_AUTO_CONTEXT=true
MEMOS_BASE_URL=http://127.0.0.1:8000
MEMOS_USER_ID=openclaw-main
MEMOS_READABLE_CUBES=openclaw-memory-operational,openclaw-memory-runtime-sync,openclaw-memory-session-reports,openclaw-memory-turn-reports,openclaw-memory-live
MEMOS_TOP_K=6
MEMOS_TIMEOUT_MS=7000

# Auto-worker (confirmed -> in_progress -> completed/failed)
AGENT_WORKER_ENABLED=false
AGENT_WORKER_RUNNER=openclaw
AGENT_WORKER_ACTOR=openclaw-worker
AGENT_WORKER_INTERVAL_MS=15000
AGENT_WORKER_BATCH_SIZE=2
AGENT_WORKER_EXEC_TIMEOUT_MS=300000
AGENT_WORKER_PROMPT_CONTEXT_CHARS=3200
AGENT_WORKER_OPENCLAW_AGENT=main
AGENT_WORKER_OPENCLAW_THINKING=low
AGENT_WORKER_OPENCLAW_LOCAL=false
AGENT_WORKER_OPENCLAW_DELIVER=false
AGENT_WORKER_OPENCLAW_TO=
AGENT_WORKER_OPENCLAW_SESSION_ID=dashboard-worker
AGENT_WORKER_OPENCLAW_TIMEOUT_SEC=240
